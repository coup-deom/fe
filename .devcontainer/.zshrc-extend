alias gs="git status"
alias ga="git add"
alias gd="git diff"
alias gb="git branch"
alias gc="git commit"
alias gsw="git switch"
alias gr="git remote"

alias gl="git log"
alias gla="git log --all"
alias glo="git log --oneline"
alias gll="git log --oneline --graph --decorate --all --source"

git config --global core.editor "nvim"
git config --global core.pager "batcat --theme=OneHalfDark"

alias tree="tre"
alias cat="batcat"
alias more="less"

